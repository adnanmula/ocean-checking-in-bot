{
    "name": "adnanmula/ocean-cheking-in-bot",
    "description": "",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sqlite3": "*",
        "beberlei/assert": "^3.3",
        "doctrine/dbal": "^2.10",
        "eleirbag89/telegrambotphp": "^1.3",
        "guzzlehttp/guzzle": "^6.5",
        "pccomponentes/ddd": "^2.3",
        "ramsey/uuid": "^4.0",
        "symfony/console": "6.*",
        "symfony/dotenv": "6.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "6.*",
        "symfony/messenger": "6.*",
        "symfony/yaml": "6.*"
    },
    "require-dev": {
        "adnanmula/coding-standard": "^0.0.5",
        "phpro/grumphp": "^1.2",
        "phpstan/phpstan": "^0.12.11",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "6.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AdnanMula\\ClockInBot\\": "src/",
            "AdnanMula\\ClockInBot\\Migrations\\": "migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdnanMula\\ClockInBot\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "rm -rf .git/hooks",
            "cp -r scripts/git-hooks .git/hooks"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.*"
        }
    }
}
